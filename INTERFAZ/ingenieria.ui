   <property name="styleSheet">
    <string notr="true">/* Barra de Menú Principal */
QMenuBar {
    background-color: #121212; /* Negro para fundirse con el fondo */
    color: #00F0FF;            /* Letras en Cian Neón para contraste */
    font-family: &quot;Segoe UI&quot;;
    font-size: 10pt;
    font-weight: bold;
    border-bottom: 1px solid #00F0FF; /* Línea divisoria neón */
}

/* Items de la barra (Archivo, Volver, etc.) */
QMenuBar::item {
    background: transparent;
    padding: 5px 15px;
}

/* Al pasar el mouse por los items superiores */
QMenuBar::item:selected {
    background-color: #00F0FF;
    color: #000000; /* Letra negra para que resalte sobre el cian */
    border-radius: 3px;
}

/* El menú desplegable (QMenu) */
QMenu {
    background-color: #1a1a1a; /* Un gris muy oscuro casi negro */
    color: #FFFFFF;            /* Letras blancas para lectura clara */
    border: 1px solid #00F0FF; /* Borde cian */
}

/* Opciones dentro del menú desplegable */
QMenu::item {
    padding: 8px 25px;
}

/* Al pasar el mouse por una opción del desplegable */
QMenu::item:selected {
    background-color: #00F0FF;
    color: #000000;
}</string>
